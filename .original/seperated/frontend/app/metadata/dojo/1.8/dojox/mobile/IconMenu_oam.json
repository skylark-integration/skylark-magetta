{
    "id": "http://dojotoolkit.org/dojox/mobile/IconMenu",
    "name": "dojox.mobile.IconMenu",
    "spec": "1.0",
    "version": "1.8",
    "require": [
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/deviceTheme",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/compat",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/IconMenu",
            "$library": "dojo"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../dojo/dojo.js"
        }
    },
    "property": {
        "transition": {
            "datatype": "string",
            "defaultValue": "slide",
            "option": [
                {"value": "slide"},
                {"value": "fade"},
                {"value": "flip"},
                {"value": "cover"},
                {"value": "coverv"},
                {"value": "dissolve"},
                {"value": "reveal"},
                {"value": "revealv"},
                {"value": "scaleIn"},
                {"value": "scaleOut"},
                {"value": "slidev"},
                {"value": "swirl"},
                {"value": "zoomIn"},
                {"value": "zoomOut"}
            ]
        },
        "iconBase": {
            "datatype": "string"
        },
        "iconPos": {
            "datatype": "string"
        },
        "cols": {
            "datatype": "number",
            "defaultValue": 3
        },
        "iconPos": {
            "datatype": "string"
        }
    },
    "content": "<ul></ul>"
}