{
    "id": "http://dojotoolkit.org/dojox/mobile/ListItem",
    "name": "dojox.mobile.ListItem",
    "spec": "1.0",
    "version": "1.7",
    "require": [
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/deviceTheme",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/compat",
            "$library": "dojo"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../dojo/dojo.js"
        }
    },
    "property": {
        "transition": {
            "datatype": "string",
            "defaultValue": "slide",
            "option": [
                {"value": "slide"},
                {"value": "fade"},
                {"value": "flip"},
                {"value": "cover"},
                {"value": "coverv"},
                {"value": "dissolve"},
                {"value": "reveal"},
                {"value": "revealv"},
                {"value": "scaleIn"},
                {"value": "scaleOut"},
                {"value": "slidev"},
                {"value": "swirl"},
                {"value": "zoomIn"},
                {"value": "zoomOut"}
            ]
        },
        "transitionDir": {
            "datatype": "number",
            "defaultValue": 1
        },
        "icon": {
            "datatype": "string"
        },
        "iconPos": {
            "datatype": "string"
        },
        "moveTo": {
            "datatype": "string"
        },
        "href": {
            "datatype": "string"
        },
        "hrefTarget": {
            "datatype": "string"
        },
        "callback": {
            "datatype": "string"
        },
        "rightText": {
            "datatype": "string"
        },
        "rightIcon": {
            "datatype": "string"
        },
        "rightIcon2": {
            "datatype": "string"
        },
        "rightIconTitle": {
            "datatype": "string"
        },
        "rightIcon2Title": {
            "datatype": "string"
        },
        "btnClass": {
            "datatype": "string"
        },
        "anchorLabel": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "noArrow": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "selected": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "checked": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "variableHeight": {
            "datatype": "boolean",
            "defaultValue": false
        }/*,
        "busy": {
            "datatype": "boolean",
            "defaultValue": false
        }*/,
        "header": {
            "datatype": "boolean",
            "defaultValue": false
        },
    },
    "content": "<li></li>"
}