<body class="claro" data-maq-flow-layout="false" data-maq-ws="collapse" data-maq-orientation="portrait" data-maq-appstates="{'states':['ds']}" id="myapp">
<div data-dojo-type="clipart.iPhone" style="position: absolute; z-index: 900; width: 457.5625px; height: 645.84375px; left: 3157px; top: -24px; display: none !important;">
  <img data-dojo-type="clipart.ArrowLeft" style="position: absolute; z-index: 900; width: 68px; height: 12px; left: 264px; top: 139px;"></img>
</div>
<div id="settings" class="view mblView">
  <script type="text/javascript">
        function setRef(id, addrRef) {
                var widget = dijit.byId("infoGroup");
                var target = dijit.byId(id);
                var model = widget.get("ref");
                target.set("ref", model[addrRef]);
            }
    </script>
    <h1 dojoType="dojox.mobile.Heading"> Data Binding Example<div label="Home" data-dojo-type="dojox.mobile.ToolBarButton" style="width: 69px; height: 24px;" toogle="false" back="false" sync="false" arrow="left"></div>
</h1>
    <form name="testForm" style="height: 115px;">
    <div class="field-title">
        Ship to - Bill to Address</div>
      <div style="width: 1008px; height: 28px;" class="spacer">
        <button data-dojo-type="dojox.mobile.Button" style="width: 41px;" class="mblBlueButton" onclick="setRef('addrGroup', 'ShipTo')">
          
        
          Ship To</button>
        <button data-dojo-type="dojox.mobile.Button" class="mblBlueButton" onclick="setRef('addrGroup', 'BillTo')">
          
        
          Bill To</button>
        <button data-dojo-type="dojox.mobile.Button" class="mblBlueButton" onclick="dijit.byId('infoGroup').get('ref').reset();">
          
        Reset</button>
      </div>
      <br></br>
      <div class="spacer"></div>
      </form>
    </div>
<span data-dojo-type="shapes.Text" style="font-size: 14px; position: absolute; z-index: 900; left: 163px; top: 180px;" content="Text"></span><a>
</a>
<input type="text" data-dojo-type="dijit.form.CurrencyTextBox" currency="USD" style="position: absolute; z-index: 900; width: 243px; left: 632px; top: 87.5px;"></input>
