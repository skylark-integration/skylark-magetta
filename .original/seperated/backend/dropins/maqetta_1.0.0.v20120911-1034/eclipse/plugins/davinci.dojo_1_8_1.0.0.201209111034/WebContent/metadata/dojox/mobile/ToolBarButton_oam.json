{
    "id": "http://dojotoolkit.org/dojox/mobile/ToolBarButton",
    "name": "dojox.mobile.ToolBarButton",
    "spec": "1.0",
    "version": "1.7",
    "require": [
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/deviceTheme",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/compat",
            "$library": "dojo"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../dojo/dojo.js"
        }
    },
    "property": {
        "transition": {
            "datatype": "string",
            "defaultValue": "slide",
            "option": [
                {"value": "slide"},
                {"value": "fade"},
                {"value": "flip"},
                {"value": "cover"},
                {"value": "coverv"},
                {"value": "dissolve"},
                {"value": "reveal"},
                {"value": "revealv"},
                {"value": "scaleIn"},
                {"value": "scaleOut"},
                {"value": "slidev"},
                {"value": "swirl"},
                {"value": "zoomIn"},
                {"value": "zoomOut"}
            ]
        },
        "transitionDir": {
            "datatype": "number",
            "defaultValue": 1
        },
        "icon": {
            "datatype": "string"
        },
        "iconPos": {
            "datatype": "string"
        },
        "moveTo": {
            "datatype": "string"
        },
        "href": {
            "datatype": "string"
        },
        "hrefTarget": {
            "datatype": "string"
        },
        "callback": {
            "datatype": "string"
        },
        "label": {
            "datatype": "string",
            "defaultValue": "ToolBarButton"
        },
        "selected": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "url": {
            "datatype": "string",
        },
        "urlTarget": {
            "datatype": "string",
        },
        "sync": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "back": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "toogle": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "arrow": {
            "datatype": "string",
            "defaultValue": "",
            "option": [
                {"value": ""},
                {"value": "right"},
                {"value": "left"}
            ]
        }
    },
    "content": "<div label='ToolBarButton'></div>"
}