{
    "id": "http://dojotoolkit.org/dojox/mobile/ScrollableView",
    "name": "dojox.mobile.ScrollableView",
    "spec": "1.0",
    "version": "1.7",
    "require": [
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/deviceTheme",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/compat",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojo/hash",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/ScrollableView",
            "$library": "dojo"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../dojo/dojo.js"
        }
    },
    "property": {
        "selected": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "keepScrollPos": {
            "datatype": "boolean",
            "defaultValue": true
        },
        "scrollBar": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "scrollDir": {
            "datatype": "string",
            "defaultValue": "v"
        },
        "fadeScrollBar": {
            "datatype": "boolean",
            "defaultValue": true
        },
        "disableFlashScrollBar": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "threshold": {
        	"datatype": "number",
        	"defaultValue": 4	
        }
    },
    "content": "<div></div>"
}