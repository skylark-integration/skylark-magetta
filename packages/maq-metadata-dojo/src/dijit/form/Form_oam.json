{
    "id": "http://dojotoolkit.org/dijit/form/Form",
    "name": "dijit.form.Form",
    "spec": "1.0",
    "version": "1.0",
    "require": [
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dijit/form/Form",
            "$library": "dojo"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../dojo/dojo.js"
        }
    },
    "property": {
        "name": {
            "datatype": "string",
            "title": "Name"
        },
        "action": {
            "datatype": "string",
            "description": "Server-side form handler.",
            "title": "Action"
        },
        "method": {
            "datatype": "string",
            "description": "HTTP method used to submit the form, either 'GET' or 'POST'.",
            "option": [
                {
                    "value": "POST"
                },
                {
                    "value": "GET"
                }
            ],
            "title": "Method"
        },
        "encType": {
            "datatype": "string",
            "description": "Encoding type for the form, ex: application/x-www-form-urlencoded.",
            "option": [
                {
                    "value": "multipart/form-data"
                }
            ],
            "title": "Encoding Type"
        },
        "accept-charset": {
            "datatype": "string",
            "description": "List of supported charsets. - not used in test files",
            "title": "Accept Charset",
            "hidden": true
        },
        "accept": {
            "datatype": "string",
            "description": "List of MIME types for file upload. - not used in test files",
            "title": "Accept",
            "hidden": true
        },
        "target": {
            "datatype": "string",
            "description": "Target frame for the document to be opened in.",
            "option": [
                {
                    "value": "_blank"
                },
                {
                    "value": "self"
                },
                {
                    "value": "_top"
                },
                {
                    "value": "_parent"
                }
            ],
            "title": "Target"
        }
    },
    "content": "<div></div>"
}