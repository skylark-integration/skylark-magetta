{
    "id": "http://dojotoolkit.org/dojox/mobile/Heading",
    "name": "dojox.mobile.Heading",
    "spec": "1.0",
    "version": "1.7",
    "require": [
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/deviceTheme",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/compat",
            "$library": "dojo"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../dojo/dojo.js"
        }
    },
    "property": {
        "back": {
            "datatype": "string"
        },
        "href": {
            "datatype": "string"
        },
        "moveTo": {
            "datatype": "string"
        },
        "transition": {
            "datatype": "string",
            "defaultValue": "slide",
            "option": [
                {"value": "slide"},
                {"value": "fade"},
                {"value": "flip"},
                {"value": "cover"},
                {"value": "coverv"},
                {"value": "dissolve"},
                {"value": "reveal"},
                {"value": "revealv"},
                {"value": "scaleIn"},
                {"value": "scaleOut"},
                {"value": "slidev"},
                {"value": "swirl"},
                {"value": "zoomIn"},
                {"value": "zoomOut"}
            ]
        },
        "label": {
            "datatype": "string",
            "defaultValue": "Heading"
        },
        "iconBase": {
            "datatype": "string"
        },
        "fixed": {
            "datatype": "string",
            "defaultValue": "",
            "option": [
                {
                    "value": ""
                },
                {
                    "value": "top"
                },
                {
                    "value": "bottom"
                }
            ]
        }/*,
        "busy": {
            "datatype": "boolean",
            "defaultValue": false
        }*/
    },
    "content": "<h1 label='Heading'></h1>"
}